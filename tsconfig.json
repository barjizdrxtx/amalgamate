{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "components/Diseases/DiseasesManage/TextEditor.js", "components/Diseases/DiseasesManage/Table.tsx", "components/Clinics/ClinicsManage/CreateClinics/GoogleApi.js", "components/UI/Lottie/Lottie.js"],
  "exclude": ["node_modules"]
}
